<!DOCTYPE html>
<html>
  <head>
    <title>List of Users</title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>Registered Users</h1>
    <ul>
      <% availableUsers.forEach(function(user) { %>
        <li>
          <%= user.username %> (<%= user.first_name %> <%= user.last_name %>) - <%= user.email %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>